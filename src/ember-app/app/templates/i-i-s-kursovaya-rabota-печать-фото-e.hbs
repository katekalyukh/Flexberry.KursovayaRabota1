<h3 class="ui header">{{t "forms.i-i-s-kursovaya-rabota-печать-фото-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерЗаказа
      class=(if (v-get validationModel "номерЗаказа" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya-rabota-печать-фото-e.номерЗаказа-caption")
      data-test-i-i-s-kursovaya-rabota-печать-фото-e-номерЗаказа=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номерЗаказа" "message")}}
  </div>
  <div class="field" data-test-i-i-s-kursovaya-rabota-печать-фото-e-носитель="true">
    <label>{{t "forms.i-i-s-kursovaya-rabota-печать-фото-e.носитель-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-kursovaya-rabota-вид-носителя")
      value=model.носитель
      class=(if (v-get validationModel "носитель" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "носитель" "message")}}
  </div>
  <div class="field" data-test-i-i-s-kursovaya-rabota-печать-фото-e-видФото="true">
    <label>{{t "forms.i-i-s-kursovaya-rabota-печать-фото-e.видФото-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-kursovaya-rabota-выбор-вида")
      value=model.видФото
      class=(if (v-get validationModel "видФото" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "видФото" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.количество
      class=(if (v-get validationModel "количество" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya-rabota-печать-фото-e.количество-caption")
      data-test-i-i-s-kursovaya-rabota-печать-фото-e-количество=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "количество" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya-rabota-печать-фото-e.фотокиоск-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.фотокиоск
      displayAttributeName="наименование"
      autocomplete=true
      relationName="фотокиоск"
      projection="ФотокиоскL"
      title=(t "forms.i-i-s-kursovaya-rabota-печать-фото-e.фотокиоск-caption")
      readonly=readonly
      componentName="фотокиоскLookup"
      class=(if (v-get validationModel "фотокиоск" "isInvalid") "error")
      data-test-i-i-s-kursovaya-rabota-печать-фото-e-фотокиоск=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "фотокиоск" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya-rabota-печать-фото-e.клиент-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.клиент
      displayAttributeName="iD"
      autocomplete=true
      relationName="клиент"
      projection="КлиентL"
      title=(t "forms.i-i-s-kursovaya-rabota-печать-фото-e.клиент-caption")
      readonly=readonly
      componentName="клиентLookup"
      class=(if (v-get validationModel "клиент" "isInvalid") "error")
      data-test-i-i-s-kursovaya-rabota-печать-фото-e-клиент=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "клиент" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya-rabota-печать-фото-e.коррекция-caption"}}</label>
    {{flexberry-groupedit
      componentName="коррекцияGroupEdit"
      mainModelProjection=modelProjection
      modelProjection=modelProjection.attributes.коррекция
      content=model.коррекция
      readonly=readonly
      orderable=false
      class=(if (v-get validationModel "коррекция" "isInvalid") "error")
      data-test-i-i-s-kursovaya-rabota-печать-фото-e-коррекция=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "коррекция" "message")}}
  </div>
</form>
